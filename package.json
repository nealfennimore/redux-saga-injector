{
  "name": "@nfen/redux-saga-injector",
  "version": "0.0.2",
  "description": "Redux Saga utils for easier use of sagas on the server and dynamic components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/redux-saga-injector.js",
  "scripts": {
    "prebundlesize": "npm run build",
    "bundlesize": "bundlesize",
    "test": "jest --config jest.config.json",
    "clean": "rimraf dist lib es",
    "build": "npm run clean && run-s build:**",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:umd": "webpack --progress -p --config ./webpack/production",
    "analyze": "webpack --progress -p --config ./webpack/production/webpack.config.analyze.js",
    "prepublish": "npm run build",
    "lint": "eslint src"
  },
  "bundlesize": [
    {
      "path": "./dist/redux-saga-injector.js",
      "maxSize": "3.1 Kb"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nealfennimore/redux-saga-injector.git"
  },
  "keywords": [],
  "author": "Neal Fennimore",
  "license": "Fair Source License, version 0.9",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node8": "^1.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bundlesize": "^0.17.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-nfen": "^0.0.19",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.2.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.1.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.12",
    "webpack-merge": "^4.1.2"
  },
  "optionalDependencies": {}
}
