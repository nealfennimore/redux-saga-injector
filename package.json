{
  "name": "@nfen/redux-saga-injector",
  "version": "1.2.0-0",
  "description": "Redux Saga utils for easier use of sagas on the server and dynamic components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/redux-saga-injector.js",
  "scripts": {
    "prebundlesize": "npm run build",
    "bundlesize": "bundlesize",
    "test": "jest --config jest.config.json",
    "clean": "rimraf dist lib es",
    "build": "npm run clean && run-s build:**",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:umd": "webpack --progress -p --config ./webpack/production",
    "analyze": "webpack --progress -p --config ./webpack/production/webpack.config.analyze.js",
    "prepublish": "npm run build",
    "lint": "eslint src"
  },
  "bundlesize": [
    {
      "path": "./dist/redux-saga-injector.js",
      "maxSize": "3.4 Kb"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nealfennimore/redux-saga-injector.git"
  },
  "keywords": [
    "redux",
    "redux saga",
    "redux injection",
    "sagas"
  ],
  "author": "Neal Fennimore",
  "license": "Fair Source License, version 0.9",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@redux-saga/symbols": "^1.1.2",
    "@redux-saga/testing-utils": "^1.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-node8": "^1.2.0",
    "bundlesize": "^0.17.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-nfen": "^0.0.19",
    "eslint-import-resolver-babel-module": "^5.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.3",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.2.0",
    "redux": "^4.0.0",
    "redux-saga": "^1.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.1.2"
  },
  "optionalDependencies": {}
}
