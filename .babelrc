{
    "presets": [
        ["@babel/env", {
            "modules": false
        }],
        "@babel/react",
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "."
                ],
                "alias": {
                    "dist": "./dist",
                    "src": "./src"
                }
            }
        ],
        "@babel/syntax-dynamic-import",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/transform-regenerator",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties"
    ],
    "env": {
        "cjs": {
            "plugins": [
                "@babel/plugin-transform-modules-commonjs"
            ],
            "ignore": [
                "*__tests__*",
                "*__fixtures__*",
                "*__integrations__*"
            ]
        },
        "es": {
            "ignore": [
                "*__tests__*",
                "*__fixtures__*",
                "*__integrations__*"
            ]
        },
        "test": {
            "plugins": [
                "@babel/plugin-transform-modules-commonjs",
                "dynamic-import-node"
            ]
        }
    }
}
